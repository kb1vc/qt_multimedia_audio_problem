cmake_minimum_required(VERSION 3.16)
project(cons_audiodevices LANGUAGES CXX)

set(CMAKE_CXX_STANDARD 17)
set(CMAKE_CXX_STANDARD_REQUIRED ON)

find_package(Qt6 REQUIRED COMPONENTS Core Gui Multimedia Widgets)
qt_standard_project_setup()

qt_add_executable(cons_audiodevices
     cons_audiodevices.cpp
)


target_link_libraries(cons_audiodevices PUBLIC
    Qt::Core
    Qt::Multimedia
)

qt_add_ios_ffmpeg_libraries(cons_audiodevices)

